<%= form_for :project do |t| %>
	Name:<br><%= t.text_field :name %><br>
	Duration:<br><%= t.text_field :duration %><br>
	Teamsize:<br><%= t.text_field :teamsize %><br>
	Cost:<br><%= t.password_field :cost %><br>
	Domain:<br><%= t.password_field :domain %><br>
	<%= t.submit "submit" %>
<% end %>



<h1>List Of Users</h1>
<table border="1px solid">
<tr>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Email</th>
</tr>


<% @users.each do |t| %>
<tr>
	<td><%= t.FirstName %></td>
	<td><%= t.LastName %></td>
	<td><%= t.Email %></td>
	

<% end %>
</tr>	

</table>
